{
  "name": "@hyperjump/json-schema",
  "version": "0.23.2",
  "description": "A JSON Schema Validator",
  "main": "lib/index.js",
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/index.mjs"
  },
  "scripts": {
    "clean": "xargs -a .gitignore rm -rf",
    "lint": "eslint lib",
    "test": "mocha --require ts-node/register 'lib/**/*.spec.ts'",
    "build": "rollup --config rollup.config.js",
    "prepublishOnly": "npm run build",
    "postinstall": "node -e \"require('fs').rmSync('dist', { recursive: true })\""
  },
  "repository": "github:hyperjump-io/json-schema-validator",
  "keywords": [
    "JSON Schema",
    "draft-04",
    "draft-06",
    "draft-07",
    "draft 2019-09",
    "draft 2020-12",
    "vocabulary",
    "vocabularies"
  ],
  "author": "Jason Desrosiers <jdesrosi@gmail.com>",
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jdesrosiers"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "*",
    "@rollup/plugin-node-resolve": "*",
    "@types/chai": "*",
    "@types/mocha": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-import-resolver-node": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "json-schema-test-suite": "github:json-schema-org/JSON-Schema-Test-Suite",
    "mocha": "*",
    "rimraf": "*",
    "rollup": "*",
    "rollup-plugin-terser": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@hyperjump/json-schema-core": "^0.28.0",
    "fastest-stable-stringify": "^2.0.2"
  }
}
